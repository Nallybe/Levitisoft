  <style>
    #carousel {
      position: relative;
      width: 100%;
      height: 350px;
      overflow: hidden;
      bottom: 20px;
      border-radius: none;
    }

    #carousel img {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      opacity: 0;
      transition: opacity 1s ease-in-out;
    }

    #carousel img.show {
      opacity: 1;
    }

    #prev-btn,
    #next-btn {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      padding: 10px 20px;
      border: none;
      background-color: transparent;
      color: #fff;
      cursor: pointer;
    }

