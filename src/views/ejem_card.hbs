                title: 'La cantidad es un campo obligatorio'
            })
        } else {
            Swal.fire({
                icon: 'success',
                title: 'Producto agregado correctamente'
            }).then(function () {
                window.location.href = "produccion_editar_iniciado";
            })
        }

    }


    //Alertas
